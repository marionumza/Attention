                                            <td class="text-right">
                                                <span t-field="line.price_subtotal"
                                                      t-options="{'widget': 'monetary', 'display_currency': res_company.currency_id}"/>
                                                <t t-set="total_before_vat"
                                                   t-value="total_before_vat+line.price_subtotal"/>

                                            </td>
                                            <td class="text-right">
